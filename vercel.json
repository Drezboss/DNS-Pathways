{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/holiday-caravan",
      "dest": "/holiday-caravan.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/get-support",
      "dest": "/get-support.html"
    },
    {
      "src": "/resources",
      "dest": "/resources.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}